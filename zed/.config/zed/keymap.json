[
  {
    "context": "Editor && jupyter && !ContextEditor",
    "bindings": {
      "ctrl-shift-enter": "repl::Run",
      "cmd-shift-backspace": "repl::ClearOutputs",
      "cmd-shift+enter": [
        "workspace::SendKeystrokes",
        "# space % % enter backspace backspace"
      ]
    }
  },
  {
  "context": "Editor",
    "bindings": {
      "ctrl-shift-b": [
        "workspace::SendKeystrokes",
        "import space pdb; space pdb.set_trace()"
      ],
      "cmd-shift-c": "editor::CopyPermalinkToLine",
      "cmd-k cmd-d": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-k cmd-r": "workspace::ToggleRightDock",
      "cmd-k cmd-d": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-i": "editor::Hover"
    }
  }
]
